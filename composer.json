{
    "name": "mertcanureten/laravel-postman-collection",
    "description": "A Laravel package to generate Postman collections from API routes.",
    "type": "library",
    "require": {
        "php": "^7.3|^8.2",
        "laravel/framework": "^9.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mertcanureten\\LaravelPostmanCollection\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mertcanureten\\LaravelPostmanCollection\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
